   "source": [
    "def potential(x,y,k=50,g=9.81)\n",
    "    gravity = sum(g*y)\n",
    "    \n",
    "    x_ = [(x[i+1]-x[i])**2 for i in range(len(x)-1)]\n",
    "    y_ = [(y[i+1]-y[i])**2 for i in range(len(y)-1)]\n",
    "    harmonic = k*(sum(x_)+sum(y_))\n",
    "    \n",
    "    return potential = gravity + harmonic"
   ]
